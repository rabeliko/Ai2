# ======================
# ÏÑ∏ÏÖò ÏÉÅÌÉú
# ======================
if "img_bytes" not in st.session_state:
    st.session_state.img_bytes = None
if "last_prediction" not in st.session_state:
    st.session_state.last_prediction = None

# ======================
# Î™®Îç∏ Î°úÎìú
# ======================
FILE_ID = st.secrets.get("GDRIVE_FILE_ID", "1uj2lD8goJDLo9uSg_8HcT4bxnl2trPc8")
MODEL_PATH = st.secrets.get("MODEL_PATH", "model.pkl")

@st.cache_resource
def load_model_from_drive(file_id: str, output_path: str):
    if not os.path.exists(output_path):
        url = f"https://drive.google.com/uc?id={file_id}"
        gdown.download(url, output_path, quiet=False)
    return load_learner(output_path, cpu=True)

with st.spinner("ü§ñ Î™®Îç∏ Î°úÎìú Ï§ë..."):
    learner = load_model_from_drive(FILE_ID, MODEL_PATH)
st.success("‚úÖ Î™®Îç∏ Î°úÎìú ÏôÑÎ£å")

labels = [str(x) for x in learner.dls.vocab]
st.write(f"**Î∂ÑÎ•ò Í∞ÄÎä•Ìïú Ìï≠Î™©:** `{', '.join(labels)}`")
st.markdown("---")

# ======================
# ÎùºÎ≤® Ïù¥Î¶Ñ Îß§Ìïë: Ïó¨Í∏∞Î•º Ï±ÑÏö∞ÏÑ∏Ïöî!
# Í∞Å ÎùºÎ≤®Îãπ ÏµúÎåÄ 3Í∞úÏî© ÌëúÏãúÎê©ÎãàÎã§.
# ======================
CONTENT_BY_LABEL: dict[str, dict[str, list[str]]] = {
    # Ïòà)
    # "Ïß¨ÎΩï": {
    #   "texts": ["Ïß¨ÎΩïÏùò ÌäπÏßïÍ≥º Ïú†Îûò", "Íµ≠Î¨º Îßõ Ìè¨Ïù∏Ìä∏", "ÏßÄÏó≠Î≥Ñ Ïä§ÌÉÄÏùº Ï∞®Ïù¥"],
    #   "images": ["https://.../jjampong1.jpg", "https://.../jjampong2.jpg"],
    #   "videos": ["https://youtu.be/XXXXXXXXXXX"]
},   
     labels[0] : ("texts" : ["Ï§ëÍµ≠Ïãù ÎÉâÎ©¥ÏùÄ ÎßõÏûàÏñ¥"], "images" : ["data:image/jpeg;base64,"]}
     labels[1]: ("texts" : ["Ïß¨ÎΩïÏùÄ ÎßõÏûàÏñ¥"], "images" : ["data:image/jpeg;base64,"]}
     labels[2] : ("texts" : ["ÏßúÏû•ÏùÄ ÎßõÏûàÏñ¥"], "images" : ["data:image/jpeg;base64,"]}
     labels[3] : ("texts" : ["ÌÉïÏàòÏú°ÏùÄ ÎßõÏûàÏñ¥"], "images" : ["data:image/jpeg;base64,"]}
},      
def yt_id_from_url(url: str) -> str | None:
    if not url: return None
    pats = [r"(?:v=|/)([0-9A-Za-z_-]{11})(?:\?|&|/|$)", r"youtu\.be/([0-9A-Za-z_-]{11})"]
    for p in pats:
        m = re.search(p, url)
        if m: return m.group(1)
    return None

def yt_thumb(url: str) -> str | None:
    vid = yt_id_from_url(url)
    return f"https://img.youtube.com/vi/{vid}/hqdefault.jpg" if vid else None

def pick_top3(lst):
    return [x for x in lst if isinstance(x, str) and x.strip()][:3]

def get_content_for_label(label: str):
    """ÎùºÎ≤®Î™ÖÏúºÎ°ú ÏΩòÌÖêÏ∏† Î∞òÌôò (texts, images, videos). ÏóÜÏúºÎ©¥ Îπà Î¶¨Ïä§Ìä∏."""
    cfg = CONTENT_BY_LABEL.get(label, {})
    return (
        pick_top3(cfg.get("texts", [])),
        pick_top3(cfg.get("images", [])),
        pick_top3(cfg.get("videos", [])),
    )

# ======================
# ÏûÖÎ†•(Ïπ¥Î©îÎùº/ÏóÖÎ°úÎìú)
# ======================
tab_cam, tab_file = st.tabs(["üì∑ Ïπ¥Î©îÎùºÎ°ú Ï¥¨ÏòÅ", "üìÅ ÌååÏùº ÏóÖÎ°úÎìú"])
new_bytes = None

with tab_cam:
    cam = st.camera_input("Ïπ¥Î©îÎùº Ïä§ÎÉÖÏÉ∑", label_visibility="collapsed")
    if cam is not None:
        new_bytes = cam.getvalue()

with tab_file:
    f = st.file_uploader("Ïù¥ÎØ∏ÏßÄÎ•º ÏóÖÎ°úÎìúÌïòÏÑ∏Ïöî (jpg, png, jpeg, webp, tiff)",
                         type=["jpg","png","jpeg","webp","tiff"])
    if f is not None:
        new_bytes = f.getvalue()

if new_bytes:
    st.session_state.img_bytes = new_bytes

# ======================
# ÏòàÏ∏° & Î†àÏù¥ÏïÑÏõÉ
# ======================
if st.session_state.img_bytes:
    top_l, top_r = st.columns([1, 1], vertical_alignment="center")

    pil_img = load_pil_from_bytes(st.session_state.img_bytes)
    with top_l:
        st.image(pil_img, caption="ÏûÖÎ†• Ïù¥ÎØ∏ÏßÄ", use_container_width=True)

    with st.spinner("üß† Î∂ÑÏÑù Ï§ë..."):
        pred, pred_idx, probs = learner.predict(PILImage.create(np.array(pil_img)))
        st.session_state.last_prediction = str(pred)

    with top_r:
        st.markdown(
            f"""
            <div class="prediction-box">
                <span style="font-size:1.0rem;color:#555;">ÏòàÏ∏° Í≤∞Í≥º:</span>
                <h2>{st.session_state.last_prediction}</h2>
                <div class="helper">Ïò§Î•∏Ï™Ω Ìå®ÎÑêÏóêÏÑú ÏòàÏ∏° ÎùºÎ≤®Ïùò ÏΩòÌÖêÏ∏†Í∞Ä ÌëúÏãúÎê©ÎãàÎã§.</div>
            </div>
            """, unsafe_allow_html=True
        )

    left, right = st.columns([1,1], vertical_alignment="top")

    # ÏôºÏ™Ω: ÌôïÎ•† ÎßâÎåÄ
    with left:
        st.subheader("ÏÉÅÏÑ∏ ÏòàÏ∏° ÌôïÎ•†")
        prob_list = sorted(
            [(labels[i], float(probs[i])) for i in range(len(labels))],
            key=lambda x: x[1], reverse=True
        )
        for lbl, p in prob_list:
            pct = p * 100
            hi = "highlight" if lbl == st.session_state.last_prediction else ""
            st.markdown(
                f"""
                <div class="prob-card">
                  <div style="display:flex;justify-content:space-between;margin-bottom:6px;">
                    <strong>{lbl}</strong><span>{pct:.2f}%</span>
                  </div>
                  <div class="prob-bar-bg">
                    <div class="prob-bar-fg {hi}" style="width:{pct:.4f}%;"></div>
                  </div>
                </div>
                """, unsafe_allow_html=True
            )

    # Ïò§Î•∏Ï™Ω: Ï†ïÎ≥¥ Ìå®ÎÑê (ÏòàÏ∏° ÎùºÎ≤® Í∏∞Î≥∏, Îã§Î•∏ ÎùºÎ≤®Î°ú Î∞îÍøîÎ≥¥Í∏∞ Í∞ÄÎä•)
    with right:
        st.subheader("ÎùºÎ≤®Î≥Ñ Í≥†Ï†ï ÏΩòÌÖêÏ∏†")
        default_idx = labels.index(st.session_state.last_prediction) if st.session_state.last_prediction in labels else 0
        info_label = st.selectbox("ÌëúÏãúÌï† ÎùºÎ≤® ÏÑ†ÌÉù", options=labels, index=default_idx)

        texts, images, videos = get_content_for_label(info_label)

        if not any([texts, images, videos]):
            st.info(f"ÎùºÎ≤® `{info_label}`Ïóê ÎåÄÌïú ÏΩòÌÖêÏ∏†Í∞Ä ÏïÑÏßÅ ÏóÜÏäµÎãàÎã§. ÏΩîÎìúÏùò CONTENT_BY_LABELÏóê Ï∂îÍ∞ÄÌïòÏÑ∏Ïöî.")
        else:
            # ÌÖçÏä§Ìä∏
            if texts:
                st.markdown('<div class="info-grid">', unsafe_allow_html=True)
                for t in texts:
                    st.markdown(f"""
                    <div class="card" style="grid-column:span 12;">
                      <h4>ÌÖçÏä§Ìä∏</h4>
                      <div>{t}</div>
                    </div>
                    """, unsafe_allow_html=True)
                st.markdown('</div>', unsafe_allow_html=True)

            # Ïù¥ÎØ∏ÏßÄ(ÏµúÎåÄ 3, 3Ïó¥)
            if images:
                st.markdown('<div class="info-grid">', unsafe_allow_html=True)
                for url in images[:3]:
                    st.markdown(f"""
                    <div class="card" style="grid-column:span 4;">
                      <h4>Ïù¥ÎØ∏ÏßÄ</h4>
                      <img src="{url}" class="thumb" />
                    </div>
                    """, unsafe_allow_html=True)
                st.markdown('</div>', unsafe_allow_html=True)

            # ÎèôÏòÅÏÉÅ(Ïú†ÌäúÎ∏å Ïç∏ÎÑ§Ïùº)
            if videos:
                st.markdown('<div class="info-grid">', unsafe_allow_html=True)
                for v in videos[:3]:
                    thumb = yt_thumb(v)
                    if thumb:
                        st.markdown(f"""
                        <div class="card" style="grid-column:span 6;">
                          <h4>ÎèôÏòÅÏÉÅ</h4>
                          <a href="{v}" target="_blank" class="thumb-wrap">
                            <img src="{thumb}" class="thumb"/>
                            <div class="play"></div>
                          </a>
                          <div class="helper">{v}</div>
                        </div>
                        """, unsafe_allow_html=True)
                    else:
                        st.markdown(f"""
                        <div class="card" style="grid-column:span 6;">
                          <h4>ÎèôÏòÅÏÉÅ</h4>
                          <a href="{v}" target="_blank">{v}</a>
                        </div>
                        """, unsafe_allow_html=True)
else:
    st.info("Ïπ¥Î©îÎùºÎ°ú Ï¥¨ÏòÅÌïòÍ±∞ÎÇò ÌååÏùºÏùÑ ÏóÖÎ°úÎìúÌïòÎ©¥ Î∂ÑÏÑù Í≤∞Í≥ºÏôÄ ÎùºÎ≤®Î≥Ñ ÏΩòÌÖêÏ∏†Í∞Ä ÌëúÏãúÎê©ÎãàÎã§.")
